- var menu = { 'Tour': '/tour', 'Integrations': '/integrations', 'Pricing': '/pricing', 'Docs': '/docs', 'Help': 'https://help.bitpay.com/', 'Sign Up': '/dashboard/signup', 'Log In': '/dashboard/login/' };

each val, key in menu
  li
    if pageSelected === key
      a.menu-item.active(href=val, title=key)= key
    else if key === 'Log In'
      a.menu-item.login-btn(href=val, title=key)= key
    else
      a.menu-item(href=val, title=key)= key
