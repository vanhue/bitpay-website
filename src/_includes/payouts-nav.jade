- var menu = { 'Mass Payouts': '/payouts#mass-payouts', 'Marketplace Payouts': '/payouts#marketplace', 'Payroll Payouts': '/payouts#payroll' };

.border-bottom-gray
  .row
    .column.medium-7.small-centered
      ul.inline-list.subnav
        each val, key in menu
          li
            if payoutSelected === key
              a.subnav-link.white.uppercase.active(href=val title=key)= key
            else
              a.subnav-link.white.uppercase(href=val title=key)= key
